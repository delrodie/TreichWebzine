{% extends 'base.html.twig' %}

{% block body %}
    <h1>Typelifestyle</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ typeLifestyle.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ typeLifestyle.libelle }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{% if typeLifestyle.statut %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ typeLifestyle.slug }}</td>
            </tr>
            <tr>
                <th>Publiepar</th>
                <td>{{ typeLifestyle.publiePar }}</td>
            </tr>
            <tr>
                <th>Modifiepar</th>
                <td>{{ typeLifestyle.modifiePar }}</td>
            </tr>
            <tr>
                <th>Publiele</th>
                <td>{{ typeLifestyle.publieLe }}</td>
            </tr>
            <tr>
                <th>Modifiele</th>
                <td>{{ typeLifestyle.modifieLe }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('backend_typelifestyle_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('backend_typelifestyle_edit', { 'id': typeLifestyle.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
